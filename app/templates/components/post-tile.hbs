<ul>
  {{#each model as |post|}}
    <li style="border: 1px #ccc solid;list-style-type:none;padding:15px;border-radius:5px;margin-bottom:5px;margin-right:25px">
      <h2 style="border-bottom:1px #ccc solid">{{post.title}}</h2>
        <img style="width:100%;height:400px" src="{{post.image}}">
      <p>{{post.contents}}</p>
    </li>
  {{/each}}
</ul>
